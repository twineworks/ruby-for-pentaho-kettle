  <step>
    <name>Determine Min&#47;Max num</name>
    <type>TypeExitRubyStep</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
	<scripts>
		<script>
			<title>Init Variables</title>
			<body># initialize variables, please note that global variables are 
# used, since tabs do not share local variable scope

# the maximum number seen
$max_number_seen = nil

# the minimum number seen
$min_number_seen = nil

# amount of rows processed
$rows_seen = 0

</body>
			<role>INIT_SCRIPT</role>
		</script>
		<script>
			<title>Row Script</title>
			<body>
num = $row[&quot;num&quot;]

$rows_seen += 1

$max_number_seen ||= num
$max_number_seen = num if (num &gt; $max_number_seen) 

$min_number_seen ||= num
$min_number_seen = num if (num &lt; $min_number_seen)

# evaluate to nil, do not produce any output row
# the output row is generated by the end script
nil</body>
			<role>ROW_SCRIPT</role>
		</script>
		<script>
			<title>Generate Output Row</title>
			<body># manually write an output row to the stream

output_row = {
	&quot;rows_seen&quot; =&gt; $rows_seen,
	&quot;min_number_seen&quot; =&gt; $min_number_seen,
	&quot;max_number_seen&quot; =&gt; $max_number_seen
}

$output.write(output_row)</body>
			<role>DISPOSE_SCRIPT</role>
		</script>
	</scripts>
