	<scripts>
		<script>
			<title>Ruby Script</title>
			<body>##################################################################################
# Ruby Scripting Step for Kettle 
# Created by Slawomir Chodnicki 
# http:&#47;&#47;type-exit.org
##################################################################################
# This script uses the rest-client gem (https:&#47;&#47;github.com&#47;archiloque&#47;rest-client)
# to read an XML response from Twitter. It queries the last 20 status updates
# from the jruby Twitter account.
##################################################################################

require &apos;rubygems&apos;
require &apos;rest_client&apos;

response = 	RestClient::get &quot;http:&#47;&#47;api.twitter.com&#47;1&#47;statuses&#47;user_timeline.xml&quot;, {
				:params =&gt; {
					:screen_name =&gt; &apos;jruby&apos;,
					:count =&gt; 20
				}
			}

# evaluate to the row with the result
{
	&quot;twitter_xml&quot; =&gt; response.to_s
}</body>
			<role>ROW_SCRIPT</role>
		</script>
