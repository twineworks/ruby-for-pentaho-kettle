	<scripts>
		<script>
			<title>Ruby Script</title>
			<body># this script joins all values read from the info step
# to the passing rows cycling the values around

# initialize by reading all values from the info step
cycles ||= $info_steps[&quot;cycles&quot;].read_all.map {|x| x[&quot;value&quot;]}

# idx is the index of the value to join
idx ||= 0

# join the value to the passing row
$row[&quot;value&quot;] = cycles[idx]

# adjust index for the next row
idx += 1
idx = 0 if idx &gt;= cycles.size

# pass on the row
$row</body>
			<role>ROW_SCRIPT</role>
		</script>
	</scripts>
	<rubyVersion>RUBY_1_8</rubyVersion>
